// Modal para agregar el Proyecto Owner.
div(id="agregarProjectOwner" class="modal hide fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true")
  div(class="modal-header")
    button(type="button" class="close" data-dismiss="modal" aria-hidden="true") Ã—
    h3 Agregar Project Owner
  div(class="modal-body")
    div(class="widget widget-table action-table")
      div(class="widget-header") 
        i(class="icon-th-list")
        h3 Lista de candidatos
      div(class="widget-content")
        table(class="table table-striped table-bordered")
          thead
            tr
              th Nombre completo 
              th(class="td-actions") Agregar
          tbody
            tr
              td Nombre del Project Owner 
              td(class="td-actions")
                a(href="#" class="btn btn-small btn-success")
                  i(class="btn-icon-only icon-ok")
  div(class="modal-footer")
    button(class="btn" data-dismiss="modal" aria-hidden="true") Cancelar
    input(type="submit" class="btn btn-primary" value="Guardar" name="savePO")

script.
  $(window).load(function() {
    $('#agregarProjectOwner').modal('show');
  });
